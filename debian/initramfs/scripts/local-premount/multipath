#!/bin/sh

set -e

PREREQ=""

prereqs() { echo "$PREREQ"; }

case $1 in
prereqs)
	prereqs
	exit 0
	;;
esac

. /scripts/functions

if [ -x /usr/sbin/multipath ]
then
	[ "$quiet" != "y" ] && log_begin_msg "Waiting for udev to settle (multipath)"
	udevadm settle --timeout=121 || true
	[ "$quiet" != "y" ] && log_end_msg
fi

multipath -r -v0 -B

exit 0
